<Window x:Class="Jumpshare.Webcam.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Jumpshare.Webcam"
        mc:Ignorable="d" WindowStartupLocation="CenterScreen"
        Title="AForge Camera" Height="400" Width="500">
    <Grid>
        <Ellipse x:Name="WebCamPlayerCircleContainer" Tag="WebCamPlayer" Width="300" Height="300" RenderTransformOrigin="0.5,0.5">
            <Ellipse.Fill>
                <ImageBrush x:Name="WebCamPlayerCircle" Stretch="UniformToFill" />
            </Ellipse.Fill>
        </Ellipse>
    </Grid>
</Window>
